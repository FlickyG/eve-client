def purge_inv_types():
	location1 = "/home/adam/Documents/eve/native/eve.db"
	conn = sqlite3.connect(location1)
	curr = conn.cursor()
	curr.execute("SELECT typeID, typeName FROM invTypes")
	x = curr.fetchall()
	for y in x:
		print (y)
	
	
