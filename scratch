sqlite> .schema invMetaTypes
CREATE TABLE "invMetaTypes" (
  "typeID" integer NOT NULL,
  "parentTypeID" integer DEFAULT NULL,
  "metaGroupID" integer DEFAULT NULL,
  PRIMARY KEY ("typeID")
);


select * from invmetaGroups limit 10;
