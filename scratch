CREATE TABLE corpassets
(itemid INT PRIMARY KEY  NOT NULL,
locationid     INT    NOT NULL,
typeid    INT    NOT NULL,
quantity    INT    NOT NULL,
flag    INT    NOT NULL,
singleton    INT    NOT NULL
);
	